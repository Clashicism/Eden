{
  "name": "gateway.cash",
  "version": "0.1.0",
  "description": "Payment Processor for Bitcoin Cash",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatewaycash/gateway.git"
  },
  "private": true,
  "workspaces": [
    "modules/*"
  ],
  "keywords": [
    "bitcoin",
    "cash"
